#nop 过迷宫,指令格式mazeout {n|s|e|w};
#alias {mazeout}
{
	#if {"%1"=="n" | "%1"=="s" | "%1"=="e" | "%1"=="w"}
	{
		#show {<afa>开始过迷宫。};
		#class mazeout open;
		#var mazeout 1;
		#ticker mazeout
		{
			#switch $mazeout
			{
				#case 1 {%1;#show %1};
				#case 2 {%1u;#show %1u};
				#case 3 {%1d;#show %1d};
			};
			#math mazeout {$mazeout+1};
			#if {$mazeout>3} {#var mazeout 1};
		} {6};
		#act {^在你筋疲力尽之际，终于找到了出路(out)。$} {#unticker mazeout;#class mazeout kill;#delay 6 out};
		#class mazeout close;
	};
	#else {#show {请指定正确的方向，格式为mazeout [dir]。}};
};

